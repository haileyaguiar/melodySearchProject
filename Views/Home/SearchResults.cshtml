@model List<MeiFile>

<h2>Search Results</h2>

@if (Model.Count == 0)
{
    <p>No results found.</p>
}
else
{
    <ul>
        @foreach (var file in Model)
        {
            <li>
                <a href="@Url.Action("DisplayFile", "Home", new { id = file.file_id })">@file.file_name</a>
            </li>
        }
    </ul>
}
